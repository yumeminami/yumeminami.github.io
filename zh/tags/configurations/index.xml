&lt;td>No&lt;/td>
&lt;td>icon photo of the website&lt;/td>
&lt;td>&lt;code>&amp;quot;/images/avatar.png&amp;quot;&lt;/code>&lt;/td>
&lt;td>&lt;code>&amp;quot;/images/avatar.png&amp;quot;&lt;/code>&lt;/td>
&lt;/tr>
&lt;tr>
&lt;td>params.options.showDarkMode&lt;/td>
&lt;td>bool&lt;/td>
&lt;td>No&lt;/td>
